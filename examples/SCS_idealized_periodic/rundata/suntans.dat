########################################################################
#
#  Input file for SUNTANS.
#
########################################################################
Nkmax			50	# Number of cells in the vertical
maxFaces		4	# Quad grid
interp          1   # interpolation method
stairstep		0	# 1 if stair-stepping, 0 if partial-stepping
rstretch		1.08	# Stretching factor for vertical grid (1<=rstretch<1.1)
CorrectVoronoi		0	# Whether or not to correct Voronoi points
VoronoiRatio		0	# Adjust the voronoi points by this amount if 1 then = centroid.
vertgridcorrect 	0 	# Correct vertical grid if Nkmax is too small
IntDepth 		1	# 1 if interpdepth, 0 otherwise
dzsmall			0.1	# Smallest grid spacing ratio before correction
scaledepth 		0 	# Scale the depth by scalefactor
scaledepthfactor 	0 	# Depth scaling factor (to test deep grids with explicit methods)
fixdzz			0	# Don't fix dzz
thetaramptime	        44712  	# Timescale over which theta is ramped from 1 to theta (fs theta only)
beta			0	# Expansivity of salt
depthelev               0       #
theta			0.55	# 0: fully explicit, 1: fully implicit
thetaS			0.55	# For scalar advection
thetaB			0.55	# For scalar advection
kappa_s                 0       # Vertical mass diffusivity
kappa_sH                0       # Horizontal mass diffusivity
gamma 			2.1e-4 	# Coefficient of expansivity of temperature.
kappa_T                 0       # Vertical thermal diffusivity
kappa_TH                0       # Horizontal thermal diffusivity
nu 			1e-5 	# Laminar viscosity of water (m^2 s^-1)
nu_H 			1e-5	# Horizontal laminar viscosity of water (m^2 s^-1)
tau_T			0 	# Wind shear stress
z0T	                0.0 	# Top roughness
z0B	                0.0	# Bottom roughness
CdT	                0.0 	# Drag coefficient at surface
CdB	                0.0	# Drag coefficient at bottom (-1 means no-slip bottom)
CdW			0.0	# Drag coefficient at sidewalls
computeSediments        0       # if 1 consider sediment model 0 otherwise
subgrid                 0       #
turbmodel		0	# Turbulence model (0 for none, 1 for MY25)
dt 			50	# Time step
Cmax 			1  	# Maximum permissible Courant number
nsteps 			6350   # Number of steps
ntout   		40	# How often to output data
ntprog   		1	# How often to report progress (in %)
ntconserve 		1	# How often to output conserved data
nonhydrostatic		1	# 0 = hydrostatic, 1 = nonhydrostatic
AB			3	# AB3
cgsolver		1	# 0 = GS, 1 = CG
maxiters		1000	# Maximum number of CG iterations
qmaxiters		2000	# Maximum number of CG iterations for nonhydrostatic pressure
qprecond		2	# 1 = preconditioned, 0 = not preconditioned
epsilon			1e-10 	# Tolerance for CG convergence
qepsilon		1e-4	# Tolerance for CG convergence for nonhydrostatic pressure
resnorm			0	# Normalized or non-normalized residual
relax			1	# Relaxation parameter for GS solver.	
amp 			0.001	# amplitude
omega 			1.4e-4	# frequency
flux 			0	# flux
timescale		0	# timescale for open boundary condition
volcheck		0	# Check for volume conservation
masscheck		0	# Check for mass conservation
nonlinear		2	# No momentum advection: 0, first-order upwind: 1, Central-differencing: 2, TVD: 5 (default superbee)
grav			9.81     # Gravity
TVDtemp		        4	   # default 4 (Van Leer), 0:No TVD. 1: FO upwind, 2: Lax-Wendroff, 3: Superbee, 4: Van Leer
TVDsalt		        4
TVDmomentum	        3
laxWendroff_Vertical    0       # No Lax Wendroff in vertical
newcells		0	# 1 if adjust momentum in surface cells as the volume changes, 0 otherwise
wetdry			0       # 1 if wetting and drying, 0 otherwise
Coriolis_f              5.7e-5 # Coriolis frequency f=2*Omega*sin(phi)
sponge_distance	        65e3 # Decay distance scale for sponge layer
sponge_decay	       800 # Decay time scale for sponge layer
readSalinity		0	# Whether or not to read initial salinity profile from file InitSalinityFile
readTemperature		0	# Whether or not to read initial temperature profile from file InitTemperatureFile
MeanFlow			0 # Whether or not to maintain a mean flow in u direction
########################################################################
#
# NetCDF / Meteorological / Boundary parameters
#
########################################################################
outputNetcdf	 	1	  # Output data to netcdf format (0 - binary, 1 - netcdf)
outputNetcdfFile sun_out.nc 	# Name of the output netcdf file
metfile	windfile.nc # Input meteorological netcdf file
#metfile		Galveston_Winds_2009_UTM.nc #
starttime	20130101.120000   # Model start time string format yyyymmdd.HHMMSS
basetime 	19900101.000000   #   Base time used for reference in netcdf files
metmodel  		0  # 0 - no meteorological input; 1 - Wood et al heat flux, 2 - COARE3.0, short and longwave radiation calculated, 3 - constant flux coefficients, 4 - wind only
varmodel  		1   	  # Kriging variogram model. 0 - use inverse distance weighting, 1 - spherical model
range  			50000    # kriging range. Grid units
nugget  		0.05   	  # kriging nugget parameter
sill  			0.95       # kriging sill parameter
Lsw			0.5	  # Solar radiation extinction depth [m] (the light extinction coefficient, k = 1/Lsw)
Cda			0.0011    # Wind drag coefficient (metmodel=3 only)
Ch			0.0011    # Stanton number / sensible heat flux coefficient (metmodel=3 only)
Ce			0.0011	  # Dalton number / latent heat flux coefficient (metmodel=3 only)
netcdfBdy		0	  # 1 - Read the open boundary data from a netcdf file
netcdfBdyFile 		sun_BC.nc # Name of the boundary netcdf file
readinitialnc		0	  # 1 - Read initial conditions from a netcdf file, 0 otherwise
initialNCfile  		sun_IC.nc  # Initial condition netcdf file name
calcage        		0   # Turn on the age calculation
calcaverage		1	  # Calculate averages
ntaverage		894	  # Number of computational points to average
ntaveragestore	2    # Number of ntaverage points to store for low pass filter U_Favg, Twindow = dt*ntaverage*ntaveragestore, NOTE: keep this small to minimize memory storage points = Ne*Nk*ntaveragestore 
averageNetcdfFile	sun_AVG.nc	#
########################################################################
#
#  Idealized Wave Forcing Parameters
#
########################################################################
ideal_wave		1		# 1=run wave forcing, 0= ignore
TauL		37.26     #low pass forcing timescale (hr)
TM2			12.42       # tidal period (hr)
ULm			0.10      # barotropic mean flow (m/s)
ULz			0.00      # baroclinic mean flow (m/s)
UHtide		0.06    # barotropic tidal flow (m/s)
Uiw 		0.50       # baroclinic wave flow (m/s)
VLm			0.05      # y barotropic mean flow (m/s)
VLz			0.00      # y baroclinic mean flow (m/s)
Phitide		0.00   # tidal phase (rad)
Phiiw		0.00     # wave phase (rad)
alphaw		1.00      # wave transfer coefficient, if 0 compute dynamically
z_alphaw	 -135.00     # depth to use for dynamic calculation of alphaw
dW			0        # width of wavemaker region (m)
Fhat_x		1.00    # wave energy flux in x direction
Fhat_y		0.00    # wave energy flux in y direction
alpha1	    -1.4248   # stratification coefficient
alpha2		0.1831   # stratification coefficient
delta		394.4088    # depth of transition pycnocline (m)
drho		5.50     # difference in density between upper and lower (kg/m3)
h2			9.9953       # height of lower layer (m)
D 			3000        # total depth (m)
lambda 		130e3	# dominant internal wavelength (m)
########################################################################
#
#  Grid Files
#
########################################################################
pslg   twod.dat 	# Planar straight line graph (input)
points points.dat	# Vertices file (input)
edges edges.dat		# Edge file (input)
cells cells.dat		# Cell centered file (input)
nodes nodes.dat
depth depth.dat		# Depth file for interpolation (if INTERPDEPTH=1) (input)
celldata celldata.dat	# Cell-centered output (output)
edgedata edgedata.dat	# Edge-centered output (output)
vertspace vertspace.dat	# Vertical grid spacing (output)
topology topology.dat	# Grid topology data
########################################################################
#
#  Output Data Files
#
########################################################################
FreeSurfaceFile   	fs.dat
HorizontalVelocityFile 	u.dat
VerticalVelocityFile 	w.dat
SalinityFile 		s.dat
BGSalinityFile 		s0.dat
TemperatureFile		T.dat
PressureFile		q.dat
VerticalGridFile 	g.dat
ConserveFile		e.dat
ProgressFile	        step.dat
StoreFile		store.dat
StartFile		start.dat
EddyViscosityFile	nut.dat
ScalarDiffusivityFile	kappat.dat
########################################################################
#
# Input Data Files
#
########################################################################
InitSalinityFile	sinit.dat
InitTemperatureFile	Tinit.dat
########################################################################
#
# Output data to analyze time series of u(z,t) and s(z,t) at 3
# locations.
#
########################################################################
ProfileVariables	su       # Only output free surface and currents
DataLocations	dataxy.dat       # dataxy.dat contains column x-y data
ProfileDataFile	profdata.dat     # Information about profiles is in profdata.dat
ntoutProfs		5        # Output profile data every 1 time step
NkmaxProfs		0        # Only output the top 1 z-level
numInterpPoints 	1        # Output data at the nearest neighbor.
########################################################################
#
# Subgrid data
#
########################################################################
segN                  1         #number of segments of each edge to generate subgrid
disN                  500                #number of discrete points to calculate Ac and Volume (minis3)
subgridmeth           1                #1 for regular subgrid method with edge segment, 2 for simple subgrid model with sub cell center depth, 3 for user defined function to define V, Ac and fluxh profile
subgriddpint          0             # 1 for regular interpolation for depth 0 for user defined function
subgriddzfmeth        1               #
subgriddragpara       0              # whether to use bottom drag parameterization for subgrid
